{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "blocks"
    },
    "thumbnail": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "hoverThumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "salePrice": {
      "type": "decimal"
    },
    "productType": {
      "type": "string"
    },
    "shortDes": {
      "type": "blocks"
    },
    "SKU": {
      "type": "biginteger"
    },
    "pCate": {
      "type": "boolean",
      "default": true
    },
    "cate": {
      "type": "boolean",
      "default": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Status",
        "Emporio",
        "OG program",
        "Kombo",
        "Premijer +",
        "Elektroinstalacijski pribor",
        "Kutije",
        "Prijenosne priključnice",
        "Grlo",
        "Zvono",
        "Happy"
      ]
    },
    "subCate": {
      "type": "enumeration",
      "enum": [
        "Bijela boja",
        "Crna boja",
        "Nosači",
        "Okviri",
        "Modularne kutije",
        "Tinjalice"
      ]
    },
    "bestSeller": {
      "type": "boolean"
    },
    "instantDelivery": {
      "type": "boolean"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
